{
  "module_id": "rinzlerlabs:sbc-hwmonitor",
  "visibility": "public",
  "url": "https://github.com/rinzlerlabs/viam-sbc-hwmonitor",
  "description": "A collection of sensors and utilities for single board computers (SBC) like the Raspberry Pi or NVIDIA Jetson Nano. The values in Readings should not be considered stable at this time. This module is under active development.",
  "models": [
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:hwmonitor:clocks"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:hwmonitor:cpu_manager"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:hwmonitor:voltages"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:hwmonitor:temperatures"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:hwmonitor:throttling"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:hwmonitor:pwm_fan"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:hwmonitor:cpu_monitor"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:hwmonitor:gpu_monitor"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:hwmonitor:memory_monitor"
    },
    {
      "api": "rdk:component:sensor",
      "model": "rinzlerlabs:hwmonitor:process_monitor"
    },
    {
      "api":"rdk:component:sensor",
      "model": "rinzlerlabs:hwmonitor:disk_monitor"
    },
    {
      "api":"rdk:component:sensor",
      "model": "rinzlerlabs:hwmonitor:wifi_monitor"
    }
  ],
  "build": {
    "build": "make package PLATFORM=linux/amd64 BUILD_TAGS=full_nvidia_support",
    "path": "package/rinzlerlabs-sbc-hwmonitor_linux_amd64.tar.gz",
    "arch": ["linux/amd64"]
  },
  "entrypoint": "bin/rinzlerlabs-sbc-hwmonitor",
  "first_run": ""
}
